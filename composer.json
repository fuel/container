{
    "name": "fuel/container",
    "type": "library",
    "description": "A fast and intuitive dependency injection container.",
    "keywords": [
        "fuel",
        "fuelphp",
        "container",
        "dependency",
        "injection",
        "di",
        "service",
        "provider"
    ],
    "homepage": "https://github.com/fuel/container",
    "license": "MIT",
    "authors": [
        {
            "name": "Phil Bennett",
            "email": "mail@philbennett.co.uk",
            "role": "Original developer"
        },
        {
            "name": "Fuel PHP team",
            "email": "contact@fuelphp.org",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/fuel/container/issues",
        "source": "https://github.com/fuel/container",
        "docs": "https://fuelphp.org/docs/container"
    },
    "require": {
        "php": "^8.2",
        "psr/container": "^1.1 || ^2.0"
    },
    "require-dev": {
        "nette/php-generator": "^3.4",
        "nikic/php-parser": "^4.10",
        "phpstan/phpstan": "^0.12.47",
        "phpunit/phpunit": "^8.5.17",
        "roave/security-advisories": "dev-latest",
        "scrutinizer/ocular": "^1.8",
        "squizlabs/php_codesniffer": "^3.6"
    },
    "provide": {
        "psr/container-implementation": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Fuel\\Container\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Fuel\\Container\\Tests\\": "tests"
        },
        "files": [
            "tests/Asset/function.php"
        ]
    },
    "extra": {
    },
    "scripts": {
        "test": [
            "phpunit",
            "phpstan analyse"
        ]
    }
}
